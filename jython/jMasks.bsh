import rmlimage.plugin.ebsd.python.jMasks;
import rmlimage.kernel.MapMath;

ByteMap map = getSelectedMap();
BinMap mask = new jMasks().discMask(map.width, map.height, map.width/2, map.height/2, map.height/2);
ByteMap result = new ByteMap(map.width, map.height);

MapMath.and(map, mask, result);

RMLImage.add(result);
